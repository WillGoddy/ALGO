function insertionSort(arr) {
    for (let i = 1; i < arr.length; i++) {
        let key = arr[i]; 
        let j = i - 1;    

        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j]; 
            j = j - 1;           
        }
        
        arr[j + 1] = key;
    }

    return arr; 
}

let arr = [5, 2, 9, 1, 5, 6];

console.log("Tableau avant le tri:", arr);


let sortedArr = insertionSort(arr);

console.log("Tableau apr√®s le tri:", sortedArr);
